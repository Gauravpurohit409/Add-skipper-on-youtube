{
    "manifest_version": 3,
    "name": "Youtube Ad skipper",
    "description": "Base Level Extension",
    "version": "1.0",
    "action": {
      "default_popup": "hello.html",
      "default_icon": "hello_extensions.png"
    },
    "content_scripts": [
        {
            "matches": ["https://www.youtube.com/*"],
            "js": ["youtube.js"]
        }
    ]
}
  